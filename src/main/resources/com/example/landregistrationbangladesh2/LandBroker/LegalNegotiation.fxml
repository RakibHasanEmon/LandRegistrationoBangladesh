<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.*?>

<VBox xmlns="http://javafx.com/javafx/8.0.171"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="LandBroker.LegalNegotiationController"
      spacing="10" alignment="CENTER" style="-fx-padding: 20;">
    
    <Label text="LegalNegotiation" style="-fx-font-size: 18px; -fx-font-weight: bold;" />
    <Button text="Meet Both Parties" fx:id="btnEvent1"/>
    <Button text="Draft Initial Terms" fx:id="btnEvent2"/>
    <Button text="Review Counter-Offers" fx:id="btnEvent3"/>
    <Button text="Finalize Agreement" fx:id="btnEvent4"/>
    <Button text="Incorporate Terms into Deed" fx:id="btnEvent5"/>
</VBox>
