<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.*?>

<VBox xmlns="http://javafx.com/javafx/8.0.171"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="LandBroker.AppealAssistanceController"
      spacing="10" alignment="CENTER" style="-fx-padding: 20;">
    
    <Label text="AppealAssistance" style="-fx-font-size: 18px; -fx-font-weight: bold;" />
    <Button text="Analyze Rejection Reason" fx:id="btnEvent1"/>
    <Button text="Collect Appeal Documents" fx:id="btnEvent2"/>
    <Button text="Draft Appeal Petition" fx:id="btnEvent3"/>
    <Button text="Submit Appeal" fx:id="btnEvent4"/>
    <Button text="Represent in Appeal Hearing" fx:id="btnEvent5"/>
</VBox>
