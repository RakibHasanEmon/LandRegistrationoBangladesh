<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.*?>

<VBox xmlns="http://javafx.com/javafx/8.0.171"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="LandBroker.LegalDisputeResolutionController"
      spacing="10" alignment="CENTER" style="-fx-padding: 20;">
    
    <Label text="LegalDisputeResolution" style="-fx-font-size: 18px; -fx-font-weight: bold;" />
    <Button text="Receive Complaint &amp; Documents" fx:id="btnEvent1"/>
    <Button text="Investigate Legal Claims" fx:id="btnEvent2"/>
    <Button text="Draft Legal Notice" fx:id="btnEvent3"/>
    <Button text="Represent Client in Proceedings" fx:id="btnEvent4"/>
    <Button text="Submit Court Verdict" fx:id="btnEvent5"/>
</VBox>
